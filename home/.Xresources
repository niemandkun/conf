! Cursor theme !

!Xcursor.theme: Vanilla-DMZ-AA

! Xft settings
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintsfull


! URxvt config !

URxvt.scrollBar: off
URxvt.font: xft:Terminus:size=12
!:antialias=true:hinting=true:hintstyle=hintmedium:rgba=rgb
!URxvt.letterSpace: -2
URxvt.internalBorder: 4
URxvt.selectToClipboard: true
URxvt.saveLines: 36000
URxvt.perl-ext-common: default,matcher,searchable-scrollback
URxvt.urlLauncher: /bin/firefox
URxvt.matcher.button: 1
URxvt.cursorBlink: true
URxvt.cursorColor: #87af5f
URxvt.cursorUnderline: false
URxvt.underlineColor: #4576b5
URxvt.fading: 0

! fake transparency
!URxvt.transparent: true
!URxvt.shading: 20
! true transparency (composite)
!URxvt.depth: 32
!URxvt.background: rgba:1100/1100/1100/cf00


